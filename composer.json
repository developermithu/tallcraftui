    "authors": [
        {
            "name": "developermithu",
            "email": "developermithu@gmail.com"
        }
    ],
    "require": {
        "blade-ui-kit/blade-heroicons": "^2.1"
    },
    "require-dev": {
        "laravel/pint": "^1.16"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}