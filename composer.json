{
  "name": "developermithu/tallcraftui",
  "description": "TallCraftUI is a Laravel blade UI components library built on TALL stack",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "tallcraftui",
    "laravel",
    "livewire",
    "livewire 3",
    "tailwindcss",
    "alpinejs",
    "tallstackui",
    "tallstack ui components",
    "blade components",
    "blade ui components",
    "laravel blade ui components",
    "livewire ui components",
    "livewire packages",
    "livewire ui library",
    "laravel ui library",
    "ui components"
  ],
  "authors": [
    {
      "name": "developermithu",
      "email": "developermithu@gmail.com",
      "homepage": "https://github.com/developermithu"
    }
  ],
  "homepage": "https://tallcraftui.developermithu.com",
  "require": {
    "illuminate/support": "^10.0|^11.0|^12.0",
    "blade-ui-kit/blade-heroicons": "^2.6",
    "gehrisandro/tailwind-merge-laravel": "^1.3"
  },
  "require-dev": {
    "laravel/pint": "^1.21"
  },
  "autoload": {
    "psr-4": {
      "Developermithu\\Tallcraftui\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Developermithu\\Tallcraftui\\TallCraftUiServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
